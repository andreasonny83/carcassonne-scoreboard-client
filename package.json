  "devDependencies": {
    "commitizen": "2.9.6",
    "concurrently": "3.4.0",
    "cz-conventional-changelog": "2.0.0",
    "jasmine-core": "2.4.1",
    "karma-chrome-launcher": "1.0.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-safari-launcher": "1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
